@using AdmCRRN.Models.Sessoes
@using AdmCRRN.Models.Transporte
@{
    ViewBag.Title = "Home Page";
}
<h2>@ViewBag.Message</h2>
<p>
    To learn more about ASP.NET MVC visit <a href="http://asp.net/mvc" title="ASP.NET MVC Website">
        http://asp.net/mvc</a>.
</p>
@if (ContaSession.UsuarioDoPerfilSuper())
{ 
    <h2>
        Usuario Adiministrador</h2>;
}
else
{
    if (@ContaSession.ContaNaSessao() != null)
    { 
    <h2>@ContaSession.ContaNaSessao().Instituicao.Nome</h2>
    }

}

