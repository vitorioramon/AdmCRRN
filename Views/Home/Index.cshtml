@using AdmCRRN.Controllers.Aplicacao.Sessao
@using AdmCRRN.Models.Transporte
@{
    ViewBag.Title = "Home Page";
}
<h2>@ViewBag.Message</h2>
<p>
    To learn more about ASP.NET MVC visit <a href="http://asp.net/mvc" title="ASP.NET MVC Website">
        http://asp.net/mvc</a>.
</p>
@if (SessaoUsuario.UsuarioSuperAdmin())
{ 
    <h2>
        Usuario Adiministrador</h2>;
}
else
{
    if (SessaoUsuario.Conta() != null)
    { 
    <h2>@SessaoUsuario.Conta().Instituicao.Nome</h2>
    }

}
